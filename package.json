{
  "name": "mu-prediction-contest",
  "version": "1.0.0",
  "description": "Web crawler for muscoop",
  "main": "src/app.js",
  "scripts": {
    "start": "npm run build && node lib/app",
    "start:login": "npm start -- login",
    "debug": "npm start -- debug",
    "debug:login": "npm start -- debug login",
    "eslint": "./node_modules/.bin/eslint --quiet src/**",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage --coverageReporters=text",
    "build": "babel src -d lib",
    "totals": "node scripts/totals",
    "post": "node scripts/post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewmayer515/mu-prediction-contest.git"
  },
  "author": "Andrew Mayer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewmayer515/mu-prediction-contest/issues"
  },
  "homepage": "https://github.com/andrewmayer515/mu-prediction-contest#readme",
  "jest": {
    "preset": "jest-puppeteer",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "test"
    ]
  },
  "dependencies": {
    "fast-levenshtein": "^2.0.6",
    "lodash": "^4.17.10",
    "ora": "^3.0.0",
    "prompts": "^1.1.0",
    "puppeteer": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.4.1",
    "jest-puppeteer": "^3.4.0",
    "regenerator-runtime": "^0.12.0"
  }
}
